<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="TVStation" table="TVStation" xmlns="">
    <identity name="Id" type="int" column="Id">
      <generator class="assigned" />
    </identity>
    <property name="Name" column="Name" type="string" not-null="true" length="10" />
    <property name="NetworkId" column="NetworkId" type="int" not-null="true" precision="10" />
    <property name="HasHD" column="HasHD" type="bool" not-null="true" />
    <property name="HDOnly" column="HDOnly" type="bool" not-null="true" />
    <property name="URI" column="URI" type="string" not-null="true" />
  </class>
</hibernate-mapping>