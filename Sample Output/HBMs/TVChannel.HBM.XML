<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="TVChannel" table="TVChannel" xmlns="">
    <identity name="Id" type="int" column="Id">
      <generator class="assigned" />
    </identity>
    <property name="StationId" column="StationId" type="int" not-null="true" precision="10" />
    <property name="ChannelNumber" column="ChannelNumber" type="int" not-null="true" precision="10" />
    <property name="IsHD" column="IsHD" type="bool" not-null="true" />
  </class>
</hibernate-mapping>